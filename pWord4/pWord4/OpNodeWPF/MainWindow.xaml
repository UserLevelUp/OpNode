<Window x:Class="OpNodeWPF.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:OpNodeWPF"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800" Loaded="Window_Loaded">
    <Grid>
        <DockPanel>
            <DockPanel DockPanel.Dock="Top" Height="50">
                <Label>Test1</Label>
                <Button TabIndex="1" Click="Button_Click">Test2</Button>
            </DockPanel>
            <DockPanel DockPanel.Dock="Bottom" Height="75">
                <TextBox AcceptsReturn="True"></TextBox>
            </DockPanel>
            <TreeView TabIndex="2" Name="OpNode" DockPanel.Dock="Left"></TreeView>
        </DockPanel>
    </Grid>
</Window>
